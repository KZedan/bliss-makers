<!Doctype html>
<html>
<head>
  <link rel="stylesheet" type="text/css" href="skeleton.css">

<style>

  body {
  background-image: url("bliss.jpg");
  background-position: 50% 50%;
  background-repeat: no-repeat;
}
</style>
</head>
<body>


<form action="/spaces" method="get">
  <input name="user_name" type="submit" value="Spaces"/>
</form>
<form action="/requests" method="get">
  <input name="requests" type="submit" value="Requests"/>
</form>
<form action="/logout" method="get">
  <input name="logout" type="submit" value="Logout"/>
</form>
<% @spaces.each do |space| %>
<%= space.space_name %>    <%= space.description %>    <%= space.price %>
<% end %>
</body>
